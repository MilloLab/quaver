{% set langCurrent = qv.language.locale|split('_') %}
<!DOCTYPE html>
<html lang="{{ langCurrent[0] }}">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="icon" type="image" href="//millolab.com/app/resources/img/favicon.ico.png">

    <title>{% block title %}Quaver Dashboard{% endblock %}</title>

    <!-- CSS -->
    <link href="{{ qv.css }}/bootstrap.min.css?{{ qv.randomVar }}" rel="stylesheet">
    <link href="{{ qv.css }}/admin.min.css?{{ qv.randomVar }}" rel="stylesheet">

    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
      <script src="//oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="//oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css">
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
    {% block head %}{% endblock %}
    <script src="{{ qv.js }}/bootstrap.min.js?{{ qv.randomVar }}"></script>
  </head>

  <body>
  {% block nav %}

    {% include 'admin/aNav.twig' %}

  {% endblock %}

  <div class="container-fluid">
    <div class="row">

      {% block body %}

        {% block sidebar %}

          {% include 'admin/aSidebar.twig' %}

        {% endblock %}

        {% block content %}{% endblock %}

      {% endblock %}
    </div>
  </div>

  {% block footer %}{% endblock %}
  {% block JS_Footer %}{% endblock %}
  </body>
</html>
